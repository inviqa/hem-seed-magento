{
    "require": {
        "php": ">=5.6.0",
        "inviqa/magento-symfony-container": "^1.1",
        "symfony/proxy-manager-bridge": "^2.6.13",
        "ocramius/proxy-manager": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.4",
        "bossa/phpspec2-expect": "^2.3",
        "behat/behat": "^3.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.3",
        "sensiolabs/behat-page-object-extension": "^2.1",
        "magetest/magento-phpspec-extension": "^4.0",
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "2.*",
        "pdepend/pdepend": "2.*",
        "phploc/phploc": "3.*",
        "theseer/phpdox": "0.8.*",
        "theseer/fxsl": "1.1.*",
        "phpunit/phpunit": "5.4.*",
        "covex-nn/phpcb": "1.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "MageTest\\PhpSpec\\MagentoExtension": "src",
            "": [
                "public/app/code/local",
                "public/app/code/community",
                "public/app/code/core",
                "public/app",
                "public/lib",
                "src"
            ],
            "Mage" : "public/app/code/core"
        }
    },
    "extra": {
        "magento-root-dir": "public"
    }
}
